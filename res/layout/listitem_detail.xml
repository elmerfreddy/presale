<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/AbsoluteLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/txtDetailProductName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txtDetailPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="44dp"
        android:layout_y="22dp"
        android:text="TextView"
        android:textColor="#969696" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="103dp"
        android:layout_y="22dp"
        android:text="Cantidad: " />

    <TextView
        android:id="@+id/txtDetailQuantity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="165dp"
        android:layout_y="22dp"
        android:text="TextView"
        android:textColor="#969696" />

    <TextView
        android:id="@+id/txtTransactionTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="222dp"
        android:layout_y="22dp"
        android:text="Total: " />

    <TextView
        android:id="@+id/txtDetailSubtotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="258dp"
        android:layout_y="22dp"
        android:text="TextView"
        android:textColor="#969696" />

    <TextView
        android:id="@+id/txtEditProductName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="22dp"
        android:text="Precio: " />

</AbsoluteLayout>
